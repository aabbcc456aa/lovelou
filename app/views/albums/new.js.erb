 <% if @save_result == true  %>
    alert("提交成功");
    $("#dialog_form").dialog("close");
    window.location.reload(); 
  <% elsif @save_result == false %>
      $("#error_messages").html('<%= escape_javascript render "shared/error_messages", :target => @album %>')
 <% else %>
      $("#dialog_form").html('<%= escape_javascript render :partial =>"form" %>')
      $("#dialog_form").dialog({
      title: '创建相册' ,
      height: 600,
      width :600,
      modal: true, 
      overlay: { opacity: 0.5, background: "black" },
      defaultBtn: "cancel",
      content: $("#dialog_form"),
      buttons: { "取消": function() { $(this).dialog("close"); } } 
      })
    <% end %>
