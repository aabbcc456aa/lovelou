 <% if @save_result == true  %>
  $("#dialog_form").dialog("close");
    window.location.href=  "albums/" +  <%= @album.id %> 
 <% elsif @save_result == false  %>
    alert("回答错误，请重新填写");
 <% else %>
      $("#dialog_form").html('<%= escape_javascript render :partial =>"validate" %>')
      $("#dialog_form").dialog({
      title: '请回答问题（主人设置了权限）' ,
      height: 300,
      width :550,
      modal: true, 
      overlay: { opacity: 0.5, background: "black" },
      defaultBtn: "cancel",
      content: $("#dialog_form"),
      buttons: { "取消": function() { $(this).dialog("close"); } } 
      })
    <% end %>
